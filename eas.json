
{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production"
      },
      "android": {
        "buildType": "aab"
      },
      "ios": {
        "simulator": false
      }
    },
    "production-ios": {
      "extends": "production",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "production-android": {
      "extends": "production",
      "android": {
        "resourceClass": "medium"
      }
    }
  },
  "submit": {
    "production": {},
    "preview": {
      "android": {
        "track": "internal"
      }
    }
  }
}
